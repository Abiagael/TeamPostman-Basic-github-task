		{
			"name": "Get Books",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"/**\r",
							" *\r",
							" * pm.test(\"Status code should be 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"pm.test(\"Status code name should have an OKstring\", function () {\r",
							"    pm.response.to.have.status(\"OK\");\r",
							"}); \r",
							" * \r",
							" */\r",
							"\r",
							"\r",
							"var jsonData = pm.response.json();\r",
							"console.log(jsonData)\r",
							"console.log(jsonData.id)\r",
							"console.log(jsonData.type)\r",
							"console.log(jsonData.name)\r",
							"\r",
							"pm.test(\"id should be 1\", function () {\r",
							"\r",
							"    pm.expect(jsonData.id).to.eql(1);\r",
							"});\r",
							"pm.test(\"type should be fiction\", function () {\r",
							"\r",
							"    pm.expect(jsonData.type).to.eql(\"fiction\");\r",
							"});\r",
							"\r",
							"pm.test(\"name should be \"The Russian\"\", function () {\r",
							"\r",
							"    pm.expect(jsonData.name).to.eql(\"The Russian\");\r",
							"});\r",
							"\r",
							""
						],
						"type": "text/javascript"
					}
				}
			],
